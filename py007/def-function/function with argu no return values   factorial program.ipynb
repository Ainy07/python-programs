    "def fact(n):\n",
    "    if type(n)==int:\n",
    "        f=1\n",
    "        for i in range(1,n+1):\n",
    "            f=f*i\n",
    "        print('factorial of',n,'is',f)\n",
    "    else:\n",
    "        print('please enter valid inputs')\n",
    "\n",
    "fact(5)\n",
    "\n",
    "fact('hello')"
   ]
  }
