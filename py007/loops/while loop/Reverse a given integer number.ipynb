   "source": [
    "n = int(input(\"enter number \"))\n",
    "rn = 0\n",
    "while n > 0:\n",
    "    reminder = n % 10\n",
    "    rn = (rn * 10) + reminder\n",
    "    n = n // 10\n",
    "print(\"Revere Number \", rn)"
   ]
