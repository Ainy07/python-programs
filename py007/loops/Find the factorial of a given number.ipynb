   "source": [
    "n = int(input(\"enter value \"))\n",
    "f = 1\n",
    "if n < 0:\n",
    "    print(\"Factorial does not exist for negative numbers\")\n",
    "elif n == 0:\n",
    "    print(\"The factorial of 0 is 1\")\n",
    "else:\n",
    "    for i in range(1, n + 1):\n",
    "        f = f * i\n",
    "    print(\"The factorial of\", n, \"is\", f)\n"
   ]
